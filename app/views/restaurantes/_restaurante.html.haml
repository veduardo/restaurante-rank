%tr
  %td
    = restaurante.nome
  %td
    = restaurante.endereco
  %td
    = restaurante.especialidade
  %td
    = link_to 'Mostrar', action: 'show', id: restaurante
  %td
    = link_to 'Editar', action: 'edit', id: restaurante
  %td
    = link_to 'Deletar', restaurante_path(restaurante), method: :delete